	column Cleaned_Material_Number =
			
			VAR CleanText = TRIM(Dim_Material_Plant[Material Number(PMI)])
			VAR Chars =
			    ADDCOLUMNS(
			        GENERATESERIES(1, LEN(CleanText)),
			        "ThisChar", MID(CleanText, [Value], 1)
			    )
			VAR OnlyDigits =
			    SUMX(
			        Chars,
			        IF([ThisChar] >= "0" && [ThisChar] <= "9", 0, 1)
			    ) = 0
			VAR FirstNonZeroPos =
			    MINX(
			        FILTER(Chars, [ThisChar] <> "0"),
			        [Value]
			    )
			RETURN
			IF(
			    OnlyDigits,
			    MID(CleanText, FirstNonZeroPos, LEN(CleanText)),
			    CleanText
			)
		lineageTag: 44c0aa6d-8ff0-4268-9b69-4bee86c97188
		summarizeBy: none

		annotation SummarizationSetBy = Automatic

	column New_Plant_Material = CONCATENATE(Dim_Material_Plant[Plant],CONCATENATE("/",Dim_Material_Plant[Cleaned_Material_Number]))
		lineageTag: 54cf8011-41e5-4f3d-a2a2-abc50eede025
		summarizeBy: none

