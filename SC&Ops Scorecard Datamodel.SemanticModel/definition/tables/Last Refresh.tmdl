	partition 'Last Refresh' = m
		mode: import
		queryGroup: Parameters/Misc
		source = ```
				let CurrentDate = DateTimeZone.UtcNow(), 
				CurrentDateAsDate = Date.From(CurrentDate), 
				Year = Date.Year(CurrentDateAsDate), 
				SecondSundayOfMarch = Date.StartOfWeek(#date(Year, 3, 8), Day.Sunday), 
				FirstSundayOfNovember = Date.StartOfWeek(#date(Year, 11, 1), Day.Sunday), 
				Result = 
				if 
				CurrentDateAsDate >= SecondSundayOfMarch and CurrentDateAsDate < FirstSundayOfNovember 
				then 
				#table(type table[Last Refresh=datetime], 
				{{DateTimeZone.UtcNow()+#duration(0, -4, 0, 0)}}) 
				else 
				#table(type table[Last Refresh=datetime], 
				{{DateTimeZone.UtcNow()+#duration(0, -5, 0, 0)}}), 
				#"Changed Type" = Table.TransformColumnTypes(Result,{{"Last Refresh", type datetimezone}}) 
				
				in 
				#"Changed Type"
				```

