table Measure
	lineageTag: 40405884-f649-4165-ab7d-331920f9f41c

	defaultDetailRowsDefinition = ```
			------------------------------
			-- Measure: [COGS Actual LYTD]
			------------------------------
			MEASURE 'Measure'[COGS Actual LYTD] = 
			    VAR LYTD = DIVIDE([Total Logistics LYTD], [Net Sales LYTD])
			    Return
			    [COGS Actual] - LYTD
			    , FormatString = "0.00%;-0.00%;0.00%"
			
			------------------------------------
			-- Measure: [COGS Actual LYTD_Final]
			------------------------------------
			MEASURE 'Measure'[COGS Actual LYTD_Final] = 
			    VAR kpi_ =  [COGS Actual LYTD]
			    VAR perc_ = abs(kpi_)
			    VAR Format_ = Format( perc_,"0.0%")
			    VAR Result_ = if(kpi_ < 0 , "▼ " &Format_,  "▲ " & Format_)
			    Return 
			    Result_
			
			------------------------------
			-- Measure: [COGS Actual MoM%]
			------------------------------
			MEASURE 'Measure'[COGS Actual MoM%] = 
			    VAR alldate_ = all(Dim_Date)
			    VAR YearPeriodSeq_ = value( MAX(Dim_Date[YearPeriodSeq]) )
			    VAR Filter_ = Filter(alldate_, Dim_Date[YearPeriodSeq] = YearPeriodSeq_ -1 )
			    VAR Net_PM = Calculate([COGS Actual],filter_)
			    VAR Result = DIVIDE(([COGS Actual] - Net_PM),Net_PM)
			    RETURN
			    Result
			
			-----------------------------------
			-- Measure: [COGS Actual MoM_Final]
			-----------------------------------
			MEASURE 'Measure'[COGS Actual MoM_Final] = 
			    VAR kpi_ =  [COGS Actual MoM%]
			    VAR perc_ = abs(kpi_)
			    VAR Format_ = Format( perc_,"0.0%")
			    VAR Result_ = if(kpi_ < 0 , "▼ " &Format_,  "▲ " & Format_)
			    Return 
			    Result_
			
			----------------------------
			-- Measure: [COGS Actual PY]
			----------------------------
			MEASURE 'Measure'[COGS Actual PY] = 
			    VAR alldate_ = all(Dim_Date)
			    VAR YearPeriodSeq_ = value( MAX(Dim_Date[YearPeriodSeq]) )
			    VAR Filter_ = Filter(alldate_, Dim_Date[YearPeriodSeq] = YearPeriodSeq_ -12 )
			    
			    RETURN
			    Calculate([COGS Actual],filter_)
			    , FormatString = "0.0"
			
			------------------------------
			-- Measure: [COGS Actual YoY%]
			------------------------------
			MEASURE 'Measure'[COGS Actual YoY%] = DIVIDE(([COGS Actual] - [COGS Actual PY]),[COGS Actual PY])
			
			-----------------------------------
			-- Measure: [COGS Actual YoY_Final]
			-----------------------------------
			MEASURE 'Measure'[COGS Actual YoY_Final] = 
			    VAR kpi_ =  [COGS Actual YoY%]
			    VAR perc_ = abs(kpi_)
			    VAR Format_ = Format( perc_,"0.0%")
			    VAR Result_ = if(kpi_ < 0 , "▼ " &Format_,  "▲ " & Format_)
			    Return 
			    Result_
			
			-----------------------------
			-- Measure: [COGS Actual YTD]
			-----------------------------
			MEASURE 'Measure'[COGS Actual YTD] = 
			    VAR _LastMonth = MAX(Dim_Date[P_YearMonth Number])
			    VAR _SelectedYear = MAX(Dim_Date[P_Year])
			    RETURN
			        SUMX(
			            FILTER(
			                ALL(Dim_Date),
			                Dim_Date[P_YearMonth Number] <= _LastMonth && Dim_Date[P_Year] = _SelectedYear
			            ),
			            [COGS Actual]
			        )
			    , FormatString = "0.0"
			
			-------------------------------
			-- Measure: [COGS_Actual_Color]
			-------------------------------
			MEASURE MetricSelector[COGS_Actual_Color] = 
			    VAR _sel = SELECTEDVALUE(MetricSelector[Metric], "vs PP")
			    
			    VAR kpi_value =
			        SWITCH(
			            _sel,
			            "vs PP", [COGS Actual MoM%],
			            "LYTD", [COGS Actual LYTD],
			            "vs YoY", [COGS Actual YoY%], 
			            BLANK()
			        )
			    
			    RETURN
			            IF(
			                kpi_value > 0,
			                "#D7191C",  -- red for ▼ down
			                "#1A9641"   -- green for ▲ up
			            )
			    , DisplayFolder = "Cond.format_measure"
			
			-------------------------
			-- Measure: [COGS_Actual]
			-------------------------
			MEASURE MetricSelector[COGS_Actual] = 
			    VAR _sel = SELECTEDVALUE( MetricSelector[Metric], "vs PP" )
			      RETURN
			        SWITCH(
			          _sel,
			          "vs PP", [COGS Actual MoM_Final],
			          "LYTD",               [COGS Actual LYTD],
			          "vs YoY",               [COGS Actual YoY_Final],
			          BLANK()
			        )
			    , DisplayFolder = "Switch Measures"
			```

