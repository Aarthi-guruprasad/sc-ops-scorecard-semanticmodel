	measure 'Total COGS GR L3M' =
			
			VAR _Months = 3
			VAR _LastMonth = VALUE(MAX(Dim_Date[YearPeriodSeq]))
			VAR _FirstMonth = _LastMonth - (_Months-1)
			RETURN
			    SUMX(
			        FILTER(
			            ALL( Dim_Date ),
			            Dim_Date[YearPeriodSeq] >= _FirstMonth && Dim_Date[YearPeriodSeq] <= _LastMonth
			        ),
			        [Total COGS GR]
			    )
		displayFolder: DIO
		lineageTag: d9229ae6-16d7-4dfa-aef0-7eda0ffcd68a

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure DIO =
			
			VAR _Months = 3
			VAR _TotalCOGS = CALCULATE([Total COGS GR L3M],KEEPFILTERS(Dim_Date[Is Closed] = "TRUE"))
			VAR _AvgInventory = CALCULATE([Inventory Avg L4M],KEEPFILTERS(Dim_Date[Is Closed] = "TRUE"))
			RETURN
			    DIVIDE( _AvgInventory, _TotalCOGS ) * (30.3333*_Months)
		formatString: 0
		displayFolder: DIO
		lineageTag: dd2f5f12-d99c-4f8e-a108-82e3e69dce9f

