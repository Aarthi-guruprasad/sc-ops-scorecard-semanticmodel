	measure 'KPI Status Icon (Sales Growth)' = ```
			
			VAR _kpiID = SELECTEDVALUE ( KPI_Metadata[KPI_ID] )
			
			VAR _timeMode =
			    SELECTEDVALUE ( CG_PeriodSelection[Period] )
			
			VAR _signRule =
			    IF (
			        _timeMode = "YTD",
			        LOOKUPVALUE ( KPI_Metadata[SignRule_YTD], KPI_Metadata[KPI_ID], _kpiID ),
			        LOOKUPVALUE ( KPI_Metadata[SignRule_PTD], KPI_Metadata[KPI_ID], _kpiID )
			    )
			
			-- Assign value based on KPI_ID
			VAR _value =
			    SWITCH (
			        _kpiID,
			        "FA_Lag1_vsPlan", [FA Target L1 Diff raw],
			        "FA_Lag1_vsLYTD", [FA% L1 LYTD CSCI],
			        "FA_Lag1_vsYoY", [FA% YoY L1 raw CSCI],
			        "FA_Lag1_vsPP", [FA% MoM L1 raw CSCI],
			
			        "FA_Lag2_vsPlan", [FA% Lag 2 Target Diff%],
			        "FA_Lag2_vsLYTD", CALCULATE([FA% CSCA LYTD], KEEPFILTERS(Fact_Forecast_Accuracy[LAGs]="2")),
			        "FA_Lag2_vsYoY", [FA% Lag2 YoY%],
			        "FA_Lag2_vsPP", [FA% Lag2 MoM%],
			
			        "FA_Lag4_CSCA_vsPlan", [FA% Lag 4 Target Diff%],
			        "FA_Lag4_CSCA_vsLYTD", CALCULATE([FA% CSCA LYTD], KEEPFILTERS(Fact_Forecast_Accuracy[LAGs]="4")),
			        "FA_Lag4_CSCA_vsYoY", [FA% Lag4 YoY%],
			        "FA_Lag4_CSCA_vsPP", [FA% Lag4 MoM%],
			
			        "FA_Lag4_CSCI_vsPlan", [FA Target L4 Diff raw],
			        "FA_Lag4_CSCI_vsLYTD", [FA% L4 LYTD CSCI],
			        "FA_Lag4_CSCI_vsYoY", [FA% YoY L4 raw CSCI],
			        "FA_Lag4_CSCI_vsPP", [FA% MoM L4 raw CSCI],
			
			        "Bias_Lag1_vsPlan", [Bias Target Diff%_CSCI L1],
			        "Bias_Lag1_vsLYTD", [Bias % L1 LYTD CSCI],
			        "Bias_Lag1_vsYoY", [Bias YoY % L1 Raw CSCI],
			        "Bias_Lag1_vsPP", [Bias MoM%_raw L1 CSCI],
			        
			        "Bias_Lag2_vsPlan", [Bias Lag2 Target Diff%],
			        "Bias_Lag2_vsLYTD", CALCULATE([Bias% LYTD], KEEPFILTERS(Fact_Forecast_Accuracy[LAGs]="2")),
			        "Bias_Lag2_vsYoY", [Bias Lag2 YoY%],
			        "Bias_Lag2_vsPP", [Bias Lag2 MoM%],
			
			        "Bias_Lag4_CSCA_vsPlan", [Bias Lag4 Target Diff%],
			        "Bias_Lag4_CSCA_vsLYTD", CALCULATE([Bias% LYTD], KEEPFILTERS(Fact_Forecast_Accuracy[LAGs]="4")),
			        "Bias_Lag4_CSCA_vsYoY", [Bias Lag4 YoY%],
			        "Bias_Lag4_CSCA_vsPP", [Bias Lag4 MoM%],
			
			        "Bias_Lag4_CSCI_vsPlan", [Bias Target Diff%_CSCI L4],
			        "Bias_Lag4_CSCI_vsLYTD", [Bias % L4 LYTD CSCI],
			        "Bias_Lag4_CSCI_vsYoY", [Bias YoY % L4 Raw CSCI],
			        "Bias_Lag4_CSCI_vsPP", [Bias MoM%_raw L4 CSCI],
			
			        "Service_Top_11_vsPlan", [Service Top 11 Target Diff%],
			        "Service_Top_11_vsYoY", [Service Top 11 YoY%],
			        "Service_Top_11_vsLYTD", [Service Top 11 LYTD],
			        "Service_Top_11_vsPP", [Service Top 11 MoM%],
			        "Case_fill_rate_vsPlan", [CFR Target Diff%],
			
			        "Case_fill_rate_vsPP", [CFR MoM%],
			        "Case_fill_rate_vsYoY", [CFR YoY%],
			        "Case_fill_rate_vsLYTD", [CFR LYTD],
			
			        "Service_Level_Gap_vsPlan", [Service Target Diff%],
			        "Service_Level_Gap_vsPP", [Service Rate% MoM%],
			        "Service_Level_Gap_vsYoY", [Service Rate YoY%],
			        "Service_Level_Gap_vsLYTD", [Service Rate LYTD],
			
			        "Service_Level_Total_vsPlan", [Service Target Diff%_CSCI],
			        "Service_Level_Total_vsPP", [Service Level MoM%],
			        "Service_Level_Total_vsYoY", [Service Level YoY%],
			        "Service_Level_Total_vsLYTD", [Service Level LYTD],
			        BLANK()
			    )
			RETURN
			    SWITCH (
			        TRUE(),
			        ISBLANK ( _value ), BLANK(),
			
			        // ✅ Bias Lag2 & Lag4 CSCA (-3% to +3% = green, else red)
			        _kpiID IN { "Bias_Lag2_vsPlan", "Bias_Lag2_vsLYTD", "Bias_Lag2_vsYoY", "Bias_Lag2_vsPP",
			                    "Bias_Lag4_CSCA_vsPlan", "Bias_Lag4_CSCA_vsLYTD", "Bias_Lag4_CSCA_vsYoY", "Bias_Lag4_CSCA_vsPP" },
			            IF ( _value >= -0.03 && _value <= 0.03, "🟢", "🔴" ),
			
			        // ✅ Bias Lag1 & Lag4 CSCI (-10% to +10% = green, else red)
			        _kpiID IN { "Bias_Lag1_vsPlan", "Bias_Lag1_vsLYTD", "Bias_Lag1_vsYoY", "Bias_Lag1_vsPP",
			                    "Bias_Lag4_CSCI_vsPlan", "Bias_Lag4_CSCI_vsLYTD", "Bias_Lag4_CSCI_vsYoY", "Bias_Lag4_CSCI_vsPP" },
			            IF ( _value >= -0.10 && _value <= 0.10, "🟢", "🔴" ),
			
			        // ✅ Other KPIs → normal rules
			        _signRule = "GoodIfNeg", IF ( _value < 0, "🟢", "🔴" ),
			        IF ( _value < 0, "🔴", "🟢" )
			    )
			```
