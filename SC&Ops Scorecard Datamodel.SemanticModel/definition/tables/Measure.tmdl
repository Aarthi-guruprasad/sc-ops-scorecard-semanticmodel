	measure 'LR CFR ET' = ```
			
			VAR UTC = [LR CFR]  // use the measure from your model directly
			RETURN
			IF(
			    ISBLANK(UTC),
			    BLANK(),
			    VAR Yr = YEAR(UTC)
			    VAR Mar1 = DATE(Yr,3,1)
			    VAR Mar1_Weekday = WEEKDAY(Mar1,1)
			    VAR SecondSunday_March = Mar1 + MOD(8 - Mar1_Weekday,7) + 7
			    VAR DST_Start = SecondSunday_March + TIME(2,0,0)
			
			    VAR Nov1 = DATE(Yr,11,1)
			    VAR Nov1_Weekday = WEEKDAY(Nov1,1)
			    VAR FirstSunday_Nov = Nov1 + MOD(8 - Nov1_Weekday,7)
			    VAR DST_End = FirstSunday_Nov + TIME(2,0,0)
			
			    VAR ET_Offset = IF(UTC >= DST_Start && UTC < DST_End, TIME(4,0,0), TIME(5,0,0))
			
			    RETURN
			        FORMAT(UTC + ET_Offset, "dd-MM-yyyy hh:mm tt") &
			        " " & IF(UTC >= DST_Start && UTC < DST_End, "EDT", "EST")
			)
			
			```
